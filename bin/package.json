{
  "name": "@mohaimin95/node-ts-generator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec 'npm run build && npm start' -e ts --ignore dist/",
    "dev:inspect": "nodemon --exec 'npm run build && node --inspect dist/index.js' -e ts --ignore dist/",
    "build": "rm -rf dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "tsc-alias": "^1.8.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  }
}
